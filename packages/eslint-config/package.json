{
  "name": "eslint-config",
  "private": true,
  "version": "0.1.0",
  "description": "eslint flat-config",
  "keywords": [
    "eslint",
    "config",
    "flat-config",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiolee/hexo-plugin.git",
    "directory": "packages/eslint-config"
  },
  "license": "MPL-2.0",
  "author": "uiolee (https://github.com/uiolee)",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./configs": "./dist/configs/index.js",
    "./rules": "./dist/rules/index.js",
    "./utils": "./dist/utils/index.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm tsbf",
    "check-types": "tsc --noEmit",
    "esb": "rm -rf dist && node scripts/esb.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsb": "tsc -b -v",
    "tsbf": "tsc -b -v -f",
    "tsbw": "tsc -b -v -w"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.10.0",
    "typescript-eslint": "^8.8.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3"
  },
  "peerDependencies": {
    "eslint": "^9.12.0",
    "@eslint/js": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.10.0",
    "typescript-eslint": "^8.8.0"
  },
  "peerDependenciesMeta": {
    "*": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
